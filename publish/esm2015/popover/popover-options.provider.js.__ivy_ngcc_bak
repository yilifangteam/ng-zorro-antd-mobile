import { Injectable } from '@angular/core';
export class PopoverOptions {
    constructor() {
        this.showArrow = false;
        this.mask = false;
        this.placement = 'bottom';
        this.appendToBody = false;
        this.className = '';
        this.autoClose = true;
    }
}
PopoverOptions.decorators = [
    { type: Injectable }
];
