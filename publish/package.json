{
  "name": "ng-zorro-antd-mobile",
  "version": "4.0.0",
  "description": "An enterprise-class mobile UI components based on Ant Design and Angular",
  "schematics": "./schematics/collection.json",
  "keywords": [
    "ant",
    "design",
    "ng-zorro-mobile",
    "mobile",
    "angular",
    "ui",
    "framework",
    "frontend"
  ],
  "homepage": "https://ng.mobile.ant.design",
  "repository": {
    "type": "git",
    "url": "https://github.com/NG-ZORRO/ng-zorro-antd-mobile"
  },
  "bugs": {
    "url": "https://github.com/NG-ZORRO/ng-zorro-antd-mobile/issues"
  },
  "dependencies": {
    "@angular/cdk": "^11.0.3",
    "@angular/animations": "^11.0.5",
    "@ant-design/icons-angular": "^11.0.1",
    "tslib": "^2.0.0"
  },
  "lint-staged": {
    "{src,docs}/**/*.{scss}": [
      "prettier --write",
      "git add"
    ],
    "{src,docs}/**/*.{js,ts,tsx}": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ]
  },
  "pre-commit": [
    "test"
  ],
  "main": "bundles/ng-zorro-antd-mobile.umd.js",
  "module": "fesm2015/ng-zorro-antd-mobile.js",
  "es2015": "fesm2015/ng-zorro-antd-mobile.js",
  "esm2015": "esm2015/ng-zorro-antd-mobile.js",
  "fesm2015": "fesm2015/ng-zorro-antd-mobile.js",
  "typings": "ng-zorro-antd-mobile.d.ts",
  "metadata": "ng-zorro-antd-mobile.metadata.json",
  "sideEffects": false
}
